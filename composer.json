{
    "name": "laravel-israel/demo-package",
    "description": "Laravel demo package",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaakov Dahan",
            "email": "yakidehan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelIsrael\\BitcoinRate\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "LaravelIsrael\\BitcoinRate\\BitcoinRateServiceProvider"
            ],
            "aliases": {
                "BitcoinRate": "LaravelIsrael\\BitcoinRate\\Facades\\BitcoinRate"
            }
        }
    },
    "minimum-stability": "dev"
}
